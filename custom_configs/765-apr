for i in PATCH{1..10}; do
   unset $i
done
PATCH1=""

( cat << "xEOFx"
   ./configure --prefix=/usr    \
               --disable-static \
               --with-installbuilddir=/usr/share/apr-1/build &&
   make
   #make -k test || true
   make -j1 install

xEOFx
) > tmp
