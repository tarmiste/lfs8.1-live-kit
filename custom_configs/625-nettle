PKG="nettle"
PKG_VERSION="nettle-3.3.tar.gz"
PKG_FILE="nettle-3.3.tar.gz "
URL="ftp://ftp.gnu.org/gnu/nettle/nettle-3.3.tar.gz"
MD5="10f969f78a463704ae73.39978148dbe"
for i in PATCH{1..10}; do
   unset $i
done
PATCH1=""

( cat << "xEOFx"
   ./configure --prefix=/usr &&
   make
   make check
   make -j1 install &&
   chmod   -v   755 /usr/lib/lib{hogweed,nettle}.so &&
   install -v -m755 -d /usr/share/doc/nettle-3.3 &&
   install -v -m644 nettle.html /usr/share/doc/nettle-3.3
xEOFx
) > tmp
